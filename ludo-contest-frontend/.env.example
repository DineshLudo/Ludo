# ludo-contest-frontend/.env.example
REACT_APP_API_URL=your_api_url
REACT_APP_MONGODB_URI=your_mongodb_uri
REACT_APP_JWT_SECRET=your_jwt_secret

# ludo-contest-frontend/vercel.json
{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}